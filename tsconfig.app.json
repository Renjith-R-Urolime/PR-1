/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": ["node"],
    "allowSyntheticDefaultImports": true,
    "paths": {
      "jszip": ["../node_modules/jszip/dist/jszip.min.js"]
    }
  },
  "files": [
    "src/main.ts",
    "src/typings.d.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
